{
  "name": "@segment/analytics-signals",
  "version": "2.2.0",
  "license": "Twilio Software Development Kit License Agreement 2.0",
  "main": "./dist/cjs/index.js",
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": [
    "./dist/umd/analytics-signals.umd.js"
  ],
  "jsdelivr": "./dist/umd/analytics-signals.umd.js",
  "unpkg": "./dist/umd/analytics-signals.umd.js",
  "files": [
    "LICENSE",
    "dist/",
    "src/",
    "!**/__tests__/**",
    "!**/test-helpers/**",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    ".": "yarn run -T turbo run --filter=$npm_package_name...",
    "..": "yarn run -T turbo run --filter=...$npm_package_name...",
    "test": "yarn jest",
    "lint": "yarn run -T biome check .",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs && yarn build:bundle",
    "build:esm": "yarn run -T tsc -p tsconfig.build.json",
    "build:cjs": "yarn run -T tsc -p tsconfig.build.json --outDir ./dist/cjs --module commonjs",
    "build:bundle": "NODE_ENV=production yarn webpack",
    "workerbox": "node scripts/build-workerbox.js",
    "assert-generated": "sh scripts/assert-workerbox-built.sh",
    "watch": "rm -rf dist && yarn build:esm --watch",
    "version": "sh scripts/version.sh",
    "watch:test": "yarn test --watch",
    "jest": "yarn run -T jest",
    "typecheck": "yarn run -T tsc --noEmit",
    "webpack": "webpack"
  },
  "dependencies": {
    "@segment/analytics-generic-utils": "1.2.0",
    "@segment/analytics-signals-runtime": "2.0.0",
    "@segment/signals-types": "2.1.0",
    "idb": "^8.0.0",
    "tslib": "^2.4.1"
  },
  "peerDependencies": {
    "@segment/analytics-next": ">=1.78.0"
  },
  "peerDependenciesMeta": {
    "@segment/analytics-next": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.9.2",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.22.1",
    "esbuild": "^0.25.6",
    "fake-indexeddb": "^6.0.0",
    "node-fetch": "^3.3.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "whatwg-fetch": "^3.6.20"
  }
}
